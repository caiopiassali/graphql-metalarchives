  "repository": {
    "type": "git",
    "url": "https://github.com/caiopiassali/graphql-metalarchives.git"
  },
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/graphql": "^6.5.1",
    "@nestjs/platform-express": "^6.7.2",
    "apollo-server-express": "^2.14.2",
    "axios": "^0.21.1",
    "axios-extensions": "^3.0.6",
    "cheerio": "^1.0.0-rc.3",
    "class-transformer": "^0.3.1",
